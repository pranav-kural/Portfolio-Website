<% include ./partials/header.ejs %>
<div class="section">
    <div id="center_pallet">
    <div class="jumbotron">
    <form method="post">
    <div class="form-group">
        <label class="control-label col-sm-2">Your Name: *</label>
        <input name="name" required placeholder="Your Name" maxlength="50" />
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2">Email: *</label>
        <input name="email" required placeholder="you@domain.com" type="email" maxlength="100" />
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2">Phone:</label>
        <input name="phone" placeholder="xxx-xxx-xxxx" type="tel" maxlength="20" />
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2">What can I do for you? *</label>
        <textarea name="message" required placeholder="Please let me know"></textarea>
    </div>
    <!--captcha-->
    <div class="col-sm-offset-2">
        <div class="g-recaptcha" data-sitekey="6Le7mw0TAAAAAHRm53EWD-j8RoRJO0EujIut0GjD"></div>
    </div>
    <div class="col-sm-offset-2">
        <button type="submit" class="btn btn-warning">Submit</button>
    </div>
</form>
</div>
    </div>
    <i class="fa fa-chevron-down" aria-hidden="true" style="margin-top: 2em"></i>
</div>
<% include ./partials/footer.ejs %>